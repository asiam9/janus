<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://java.sun.com/xml/ns/javaee" xmlns:web="http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd" version="3.0">
  <display-name>janus</display-name>
  <env-entry>
    <env-entry-name>basePath</env-entry-name>
    <env-entry-type>java.lang.String</env-entry-type>
    <env-entry-value>D:/Media/books</env-entry-value>
  </env-entry>
  <env-entry>
    <env-entry-name>databaseName</env-entry-name>
    <env-entry-type>java.lang.String</env-entry-type>
    <env-entry-value>metadata.db</env-entry-value>
  </env-entry>
  <welcome-file-list>
    <welcome-file>index.jsp</welcome-file>
    <welcome-file>index.xml</welcome-file>
  </welcome-file-list>
</web-app>